{
    "name": "web-template",
    "version": "1.0.0",
    "description": "frontend template",
    "main": "main.js",
    "directories": {
        "doc": "doc"
    },
    "scripts": {
        "dev": "vite serve --config build/vite.config.dev.js",
        "build-test": "vite build --config build/vite.config.test.js",
        "build": "vite build --config build/vite.config.online.js",
        "analyze": "cross-env Analyze=true vite build --config build/vite.config.online.js",
        "build-sh": "sh script/build.sh"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Fantasy15/frontend-template.git"
    },
    "author": "Fe3o4",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Fantasy15/frontend-template/issues"
    },
    "homepage": "https://github.com/Fantasy15/frontend-template#readme",
    "dependencies": {
        "autoprefixer": "^9.7.5",
        "axios": "^0.27.1",
        "cross-env": "^7.0.3",
        "lodash": "^4.17.21",
        "pinia": "^2.0.13",
        "vue": "^3.2.33",
        "vue-router": "^4.0.14",
        "vue-template-compiler": "^2.6.14"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-url": "^7.0.0",
        "@vitejs/plugin-legacy": "^1.8.2",
        "@vitejs/plugin-vue": "^2.3.3",
        "rollup-plugin-visualizer": "^5.6.0",
        "vite": "^2.9.9"
    }
